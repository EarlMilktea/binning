{
  "name": "binning",
  "type": "module",
  "version": "0.1.15",
  "license": "MIT",
  "description": "Correlated data analyzer written in TypeScript",
  "keywords": [
    "Monte Carlo",
    "data analysis",
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EarlMilktea/binning.git"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "binning": "./dist/src/bin.js"
  },
  "sideEffects": [
    "./dist/src/bin.js"
  ],
  "exports": {
    ".": "./dist/src/index.js",
    "./binner.js": "./dist/src/binner.js",
    "./cli.js": "./dist/src/cli.js",
    "./data-io.js": "./dist/src/data-io.js",
    "./stats.js": "./dist/src/stats.js"
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "argparse": "^2.0.1",
    "typebox": "^1.0.78"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tsconfig/node20": "^20.1.8",
    "@types/argparse": "^2.0.17",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.1.0",
    "eslint-plugin-n": "^17.23.2",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esbuild": "jiti bundle.ts",
    "build:tsc": "tsc",
    "lint": "run-p lint:*",
    "lint:check": "eslint",
    "lint:format": "prettier --check .",
    "lint:format:write": "prettier --write .",
    "prepack": "run-s build",
    "test": "vitest run"
  }
}
