{
  "name": "binning",
  "type": "module",
  "version": "0.1.13",
  "license": "MIT",
  "description": "Correlated data analyzer written in TypeScript",
  "keywords": [
    "Monte Carlo",
    "data analysis",
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EarlMilktea/binning.git"
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "binning": "./dist/src/bin.js"
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./binner.js": "./dist/src/binner.js",
    "./cli.js": "./dist/src/cli.js",
    "./data-io.js": "./dist/src/data-io.js",
    "./stats.js": "./dist/src/stats.js"
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "argparse": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/argparse": "^2.0.17",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^57.0.8",
    "eslint-plugin-n": "^17.21.3",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esbuild": "jiti bundle.ts",
    "build:tsc": "tsc",
    "lint": "run-p lint:*",
    "lint:check": "eslint",
    "lint:format": "prettier --check .",
    "lint:format:write": "prettier --write .",
    "prepack": "run-s build",
    "test": "vitest run"
  }
}
