{
  "name": "binning",
  "type": "module",
  "version": "0.1.14",
  "license": "MIT",
  "description": "Correlated data analyzer written in TypeScript",
  "keywords": [
    "Monte Carlo",
    "data analysis",
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EarlMilktea/binning.git"
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "binning": "./dist/src/bin.js"
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./binner.js": "./dist/src/binner.js",
    "./cli.js": "./dist/src/cli.js",
    "./data-io.js": "./dist/src/data-io.js",
    "./stats.js": "./dist/src/stats.js"
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "argparse": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/argparse": "^2.0.17",
    "@types/node": "^25.0.2",
    "@vitest/coverage-v8": "^4.0.8",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-n": "^17.23.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esbuild": "jiti bundle.ts",
    "build:tsc": "tsc",
    "lint": "run-p lint:*",
    "lint:check": "eslint",
    "lint:format": "prettier --check .",
    "lint:format:write": "prettier --write .",
    "prepack": "run-s build",
    "test": "vitest run"
  }
}
